<?xml version="1.0" encoding="UTF-8"?>

<flowers xmlns="http://www.nure.ua"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.nure.ua input.xsd ">
	
	<flower>
		<name>Rose</name>
		<soil>дерново-подзолистая</soil>
		<origin>China</origin>
		<visualParameters>
			<stemColor>green</stemColor>
			<leafColor>green</leafColor>
			<aveLenFlower measure = "cm">10</aveLenFlower>
		</visualParameters>
		<growingTips>
			<temperature measure = "celcius">25</temperature>
			<lighting lightRequiring = "yes"/>
			<watering measure = "mlPerWeek">110</watering>
		</growingTips>
		<multiplying>черенки</multiplying>
	</flower>	
	
	<flower>
		<name>Bambusa</name>
		<soil>дерново-подзолистая</soil>
		<origin>China</origin>
		<visualParameters>
			<stemColor>green</stemColor>
			<leafColor>green</leafColor>
			<aveLenFlower measure = "cm">1100</aveLenFlower>
		</visualParameters>
		<growingTips>
			<temperature measure = "celcius">30</temperature>
			<lighting lightRequiring = "yes"/>
			<watering measure = "mlPerWeek">250</watering>
		</growingTips>
		<multiplying>черенки</multiplying>
	</flower>

</flowers>
